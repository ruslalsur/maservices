{
  "name": "maservices",
  "version": "1.0.0",
  "description": "servises fof Masha",
  "main": "express.js",
  "scripts": {
    "start": "NODE_ENV=production node -r dotenv/config express.js",
    "server:dev": "nodemon -r dotenv/config express.js",
    "client:dev": "npm run start --prefix client",
    "client:build": "export NODE_OPTIONS=--max_old_space_size=2048 && npm run build --prefix client",
    "client:install": "npm install --prefix client",
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^8.2.0",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "concurrently": "^6.0.2",
    "nodemon": "^2.0.7"
  }
}
